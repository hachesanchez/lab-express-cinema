<h1>MOVIE'S LIST</h1>



<div class="container">

    <div class="movies-page">

        <div class="row">
            {{#each movies}}

            <div class="col-md-4">
                <img src="{{image}}" alt="{{title}}">
                <div class="card-body">
                    <h2>{{title}}</h2>
                    <a href="/movie-details/{{_id}}">See More</a>

                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>